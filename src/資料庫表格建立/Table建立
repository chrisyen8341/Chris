CREATE TABLE DIARY (
 DIANO                  NUMBER(8) CONSTRAINT DIARY_DIANO_PK PRIMARY KEY,
 MEMNO                	NUMBER(8) NOT NULL constraint diary_mem_fk references member(memno),
 DIANAME                VARCHAR2(100) NOT NULL,
 DIATEXT           		CLOB NOT NULL,
 DIAIMG                 BLOB,
 DIACRETIME             TIMESTAMP NOT NULL,
 DIAMODTIME             TIMESTAMP,
 DIASTATE				NUMBER(1) NOT NULL );
 
 CREATE SEQUENCE DIARY_seq
INCREMENT BY 1
START WITH 10001
NOMAXVALUE
NOCYCLE
NOCACHE;
 
